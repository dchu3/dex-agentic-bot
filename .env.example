GEMINI_API_KEY=your-key-here
GEMINI_MODEL=gemini-2.5-flash

# MCP Server commands
MCP_DEXSCREENER_CMD=node /path/to/dex-screener-mcp/dist/index.js
MCP_DEXPAPRIKA_CMD=dexpaprika-mcp
MCP_HONEYPOT_CMD=node /path/to/dex-honeypot-mcp/dist/index.js
MCP_RUGCHECK_CMD=node /path/to/dex-rugcheck-mcp/dist/index.js
MCP_SOLANA_RPC_CMD=node /path/to/solana-rpc-mcp/dist/index.js
MCP_BLOCKSCOUT_CMD=node /path/to/dex-blockscout-mcp/dist/index.js
MCP_TRADER_CMD=node /path/to/dex-trader-mcp/dist/index.js

# Solana RPC URL for on-chain lookups (token decimals etc.)
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Agent settings
AGENTIC_MAX_ITERATIONS=15
AGENTIC_MAX_TOOL_CALLS=30
AGENTIC_TIMEOUT_SECONDS=120

LOG_LEVEL=INFO

# Watchlist settings
WATCHLIST_POLL_ENABLED=true
WATCHLIST_POLL_INTERVAL=60

# Alert auto-adjustment (resets thresholds after trigger)
ALERT_AUTO_ADJUST_ENABLED=true
ALERT_TAKE_PROFIT_PERCENT=10.0
ALERT_STOP_LOSS_PERCENT=5.0

# Telegram settings (optional)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
TELEGRAM_ALERTS_ENABLED=false
TELEGRAM_PRIVATE_MODE=true
TELEGRAM_SUBSCRIBERS_DB_PATH=

# Autonomous Agent settings
AUTONOMOUS_ENABLED=false
AUTONOMOUS_INTERVAL_MINS=60
AUTONOMOUS_MAX_TOKENS=5
AUTONOMOUS_CHAIN=solana
AUTONOMOUS_MIN_VOLUME_USD=10000
AUTONOMOUS_MIN_LIQUIDITY_USD=5000

# Lag strategy settings (Solana-first, trader MCP)
LAG_STRATEGY_ENABLED=false
LAG_STRATEGY_DRY_RUN=true
LAG_STRATEGY_INTERVAL_SECONDS=20
LAG_STRATEGY_CHAIN=solana
LAG_STRATEGY_SAMPLE_NOTIONAL_USD=25
LAG_STRATEGY_MIN_EDGE_BPS=30
LAG_STRATEGY_MIN_LIQUIDITY_USD=10000
LAG_STRATEGY_MAX_SLIPPAGE_BPS=100
LAG_STRATEGY_MAX_POSITION_USD=25
LAG_STRATEGY_MAX_OPEN_POSITIONS=2
LAG_STRATEGY_COOLDOWN_SECONDS=180
LAG_STRATEGY_TAKE_PROFIT_BPS=150
LAG_STRATEGY_STOP_LOSS_BPS=80
LAG_STRATEGY_MAX_HOLD_SECONDS=1800
LAG_STRATEGY_DAILY_LOSS_LIMIT_USD=50
# Optional explicit trader method names (auto-detected by default)
LAG_STRATEGY_QUOTE_METHOD=
LAG_STRATEGY_EXECUTE_METHOD=
# Default Solana quote mint is USDC
LAG_STRATEGY_QUOTE_MINT=EPjFWdd5AufqSSqeM2qN1xzybapC8G4wEGGkZwyTDt1v
# Maximum price impact percentage before skipping a trade (0 = disabled)
LAG_STRATEGY_MAX_PRICE_IMPACT_PCT=1.0
# Estimated SOL fee buffer in lamports for atomic profitability check (base + priority fees)
LAG_STRATEGY_FEE_BUFFER_LAMPORTS=20000
# Rolling expectancy gate — pauses atomic entries when avg PnL is persistently negative
LAG_STRATEGY_ATOMIC_EVAL_WINDOW=20        # number of recent atomic trades to evaluate
LAG_STRATEGY_ATOMIC_MIN_SAMPLES=10        # minimum trades before gate activates
LAG_STRATEGY_ATOMIC_PAUSE_EXPECTANCY_BPS=0.0  # pause when avg PnL bps <= this value

# Portfolio strategy settings (discover → hold → exit at TP/SL)
PORTFOLIO_ENABLED=false
PORTFOLIO_DRY_RUN=true
PORTFOLIO_CHAIN=solana
PORTFOLIO_MAX_POSITIONS=5
PORTFOLIO_POSITION_SIZE_USD=5.00
PORTFOLIO_TAKE_PROFIT_PCT=15.0
PORTFOLIO_STOP_LOSS_PCT=8.0
PORTFOLIO_TRAILING_STOP_PCT=5.0
PORTFOLIO_MAX_HOLD_HOURS=24
PORTFOLIO_DISCOVERY_INTERVAL_MINS=30
PORTFOLIO_PRICE_CHECK_SECONDS=60
PORTFOLIO_DAILY_LOSS_LIMIT_USD=50.0
PORTFOLIO_MIN_VOLUME_USD=10000
PORTFOLIO_MIN_LIQUIDITY_USD=5000
PORTFOLIO_COOLDOWN_SECONDS=300
PORTFOLIO_MIN_MOMENTUM_SCORE=50.0
PORTFOLIO_MAX_SLIPPAGE_BPS=100
